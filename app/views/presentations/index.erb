<div class="container">
  <div class="row row-content">
    <p class="navclear"></p>
  </div>
</div>

<div class="container">
  <div class="row row-content">
    <div class="col-xs-12">
      <h1 class="text-center">Presentation's Index</h1>
    </div>
  </div>
</div>

<% @presentations.each do |presentation| %>
  <div class="container">
    <div class="row row-content">
      <div class="col-xs-12 col-sm-6">
        <a href="www.google.com">
          <iframe width="534" height="300" src="<%= presentation.youtube_embedded_link %>" frameborder="0" allowfullscreen></iframe>
        </a>
      </div>
      <div class="col-xs-12 col-sm-6">

          <div class="row row-content">
            <div class="col-xs-4 col-xs-offset-2 col-sm-4 col-sm-offset-0">
              
              <div class="row row-content">
                <div class="col-xs-12">
                  <h2><%# student.first_name %></h2>
                  <h3><%# student.last_name%></h3>
                </div>
              </div>
              
            </div>
            <div class="col-xs-4 col-sm-4 col-sm-offset-4 col-md-offset-2">
              <h3 class="text-right"><%= presentation.student_name %></h3>
            </div>
          </div>
        
          <div class="row row-content">
            <div class="col-xs-12">
              <%# Maybe a well here with a quote? %>
            </div>
          </div>
        
      </div>
    </div>
  </div>
  
<% end %>


